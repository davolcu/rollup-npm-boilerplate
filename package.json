{
  "name": "rollup-npm-boilerplate",
  "version": "1.0.0",
  "description": "Simple boilerplate to start an NPM package with and bundle it with Rollup on VS Code",
  "main": "path/to/file.min.js",
  "module": "path/to/file.esm.js",
  "global": "GlobalNameOfThePackage",
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/preset-env": "^7.14.9",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.3.2",
    "rollup": "^2.55.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "start": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davolcu/rollup-npm-boilerplate.git"
  },
  "keywords": [],
